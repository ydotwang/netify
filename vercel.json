{
  "$schema": "https://openapi.vercel.sh/vercel.json",

  "functions": {
    "api/backend/main.py": {
      "maxDuration": 15
    }
  },

  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index/:path*"
    },
    {
      "source": "/backend/main/:path*",
      "destination": "/api/backend/main/:path*"
    }
  ]
}